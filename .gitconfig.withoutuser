[core]
	autocrlf = false
	editor = vim

[color]
	ui = auto

[alias]
	st = status
	sw = switch
	ci = commit
	ciam = commit --amend
	camreset = reset --soft HEAD@{1}
	cob = checkout -b
	br = branch
	swma = switch master
	coma = switch master
	a = add
	au = add -u
	d = diff
	l = log
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
